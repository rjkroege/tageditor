Edit X:tageditor/\+Errors: 1,$d
X:tageditor/.*\.swift: w

# Setup a library
swift package init

# Setup an executable
swift package init --type executable

# Build
swift build

# Test
swift test

# Run
swift run  tageditor -c Robert  foobar giant 02_Raisin_Exercise.mp3 /Users/rjkroege/wrks/docslocal/parents/audio-books/A_Courtesans_Scandal/*

swift run  tageditor -u --title 'Raisin Exercise' 02_Raisin_Exercise.mp3



# Format
swift-format -i -r Sources
Edit X/\.(swift)$/ e

# NB: binary is here.
./.build/arm*/debug/tageditor


